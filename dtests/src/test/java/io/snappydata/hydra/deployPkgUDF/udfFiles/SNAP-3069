create function myudf3 as io.snappydata.hydra.MyUDF3 returns integer using jar '/home/cbhatt/TestWork/out/artifacts/TestWork_jar/myudf3.jar';
select myudf3(25,25);
drop function myudf3;


create function myudf4 as io.snappydata.hydra.MyUDF4 returns float using jar '/home/cbhatt/TestWork/out/artifacts/TestWork_jar/myudf4.jar';
select myudf4(30);
drop function myudf4;

create function myudf5 as io.snappydata.hydra.MyUDF5 returns string using jar '/home/cbhatt/TestWork/out/artifacts/TestWork_jar/myudf5.jar';
select myudf5('snappydata');
drop function myudf5;

create function myudf6 as io.snappydata.hydra.MyUDF6 returns integer using jar '/home/cbhatt/TestWork/out/artifacts/TestWork_jar/myudf6.jar';
select myudf6(1.5d,2.7d,4.1d,5.6d);
drop function myudf6;


create function myudf7 as io.snappydata.hydra.MyUDF7 returns integer using jar '/home/cbhatt/TestWork/out/artifacts/TestWork_jar/myudf7.jar';
select myudf7(25,27);
drop function myudf7;


create table if not exists Student(id int,name string,subject string,division int,total int,percentage float,surname string,roundoff int,maximum int);
insert into table Student select id,concat('TIBCO_',id),subject(id%10),default.ID(Array('5','7','11','13','6')),myudf3(25,25),myudf4(30),myudf5('snappydata'),myudf6(1.5d,2.7d,4.1d,5.6d),myudf7(25,27) from range(1000);
select * from Student;
drop table Student;



